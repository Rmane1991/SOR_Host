        /* Initially hide the menu name */
        .menu-name {
            display: none;
            padding-left: 10px;
        }

        /* Show the menu name when hovering over the menu item */
        .menu-toggle:hover .menu-name,
        .sub-menu-list:hover .menu-name {
            display: inline-block;
            color: #000; /* You can change the color as needed */
            font-weight: bold; /* Optional, to emphasize the menu name */
        }

        /* Optional: Add a transition for a smooth effect */
        .menu-name {
            transition: opacity 0.3s ease;
        }

        /* Remove the downward arrow after the menu icon */
        .menu-toggle::after {
            content: none; /* Removes the arrow if it's added via ::after */
        }

        /* Optionally, hide the arrow icon (e.g., chevron or arrow) if it's added using Font Awesome */
        .menu-toggle .fa-chevron-down,
        .menu-toggle .fa-arrow-down {
            display: none;
