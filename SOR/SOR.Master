            document.addEventListener('DOMContentLoaded', function () {
    const sidebar = document.getElementById('sidebar');
    const sliderButton = document.getElementById('slider-btn');
    
    // Check if the current page is DashBoard.aspx
    const isDashboardPage = window.location.pathname.includes('DashBoard.aspx'); // Check for 'DashBoard.aspx' in the URL
    
    // If it's the dashboard page, close the sidebar by default and prevent the default action
    if (isDashboardPage) {
        sidebar.classList.add('closed');
        sidebar.classList.remove('open');
        sliderButton.classList.remove('rotate');
        localStorage.setItem('sidebarState', 'closed');  // Save state as closed for dashboard
        
        // Prevent any action or postback from the slider button
        sliderButton.addEventListener('click', function (event) {
            event.preventDefault(); // Prevent the default behavior (like postback)
        });
    } else {
        // If it's not the dashboard page, use the saved state from localStorage
        const sidebarState = localStorage.getItem('sidebarState');
        if (sidebarState === 'closed') {
            sidebar.classList.add('closed');
            sidebar.classList.remove('open');
            sliderButton.classList.remove('rotate');
        } else {
            sidebar.classList.add('open');
            sidebar.classList.remove('closed');
            sliderButton.classList.add('rotate');
        }

        // Toggle the sidebar when the slider button is clicked
        sliderButton.addEventListener('click', function (event) {
            event.preventDefault(); // Prevent the default behavior (like postback)
            
            if (sidebar.classList.contains('open')) {
                sidebar.classList.remove('open');
                sidebar.classList.add('closed');
                sliderButton.classList.remove('rotate');
                localStorage.setItem('sidebarState', 'closed'); // Save state as closed
            } else {
                sidebar.classList.remove('closed');
                sidebar.classList.add('open');
                sliderButton.classList.add('rotate');
                localStorage.setItem('sidebarState', 'open'); // Save state as open
            }
        });
    }
});




